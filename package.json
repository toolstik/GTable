{
  "name": "gtable",
  "version": "0.0.1",
  "description": "\"An ORM for Google Apps scripts\"",
  "repository": {
    "type": "git",
    "url": "https://github.com/toolstik/GTable.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -r -f dist",
    "build": "npm run build:src && npm run build:spec",
    "build:src": "grunt copy:src",
    "build:spec": "grunt copy:spec",
    "upload": "sh upload.sh",
    "deploy": "npm run clean && npm run build && npm run upload",
    "test": "npm run deploy && sh clasp.sh run test"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/google-apps-script": "^0.0.35",
    "grunt": "~0.4.5",
    "grunt-contrib-copy": "^1.0.0"
  }
}